{
  "rewrites": [
    {
      "source": "/rss",
      "destination": "/blog/rss"
    },
    {
      "source": "/rss.xml",
      "destination": "/blog/rss"
    },
    {
      "source": "/feed",
      "destination": "/blog/rss"
    },
    {
      "source": "/feed.xml",
      "destination": "/blog/rss"
    }
  ],
  "redirects": [
    {
      "source": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    }
  ],
  "functions": {
    "api/og/**/*.ts": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron/warmup",
      "schedule": "0 3 * * *"
    }
  ]
}